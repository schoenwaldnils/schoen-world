{
  "name": "schoen-world",
  "version": "0.0.1",
  "description": "Schoen.World",
  "main": "index.js",
  "scripts": {
    "dev": "next dev -p 3002",
    "start": "next build && next start -p 3002",
    "prebuild": "npm run generate-contentful-types",
    "build": "next build",
    "export": "NODE_ENV=production next build && next export",
    "test": "npm run lint",
    "lint": "npm run lint:ts && npm run lint:types && npm run lint:css",
    "lint:css": "stylelint '**/*.tsx'",
    "lint:ts": "eslint . --ext .ts,.tsx",
    "lint:ts-fix": "eslint . --ext .ts,.tsx --fix",
    "lint:types": "tsc",
    "lint:html": "htmlhint out",
    "generate-contentful-types": "contentful-typescript-codegen --output src/@types/generated/contentful.d.ts",
    "generate-favicons": "real-favicon generate faviconDescription.json src/components/Favicons/faviconData.json public/images/favicons",
    "storybook": "start-storybook -p 6007",
    "build-storybook": "build-storybook -o public/storybook --quiet",
    "snapshot": "npm run build-storybook && percy storybook -t=500 ./public/storybook",
    "analyze": "cross-env ANALYZE=true next build",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [
    "next.js",
    "nextjs",
    "contentful",
    "static"
  ],
  "repository": "github:schoenwaldnils/schoen-world",
  "author": {
    "name": "Nils Sch√∂nwald",
    "email": "nils@schoen.world"
  },
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.15.8",
    "@contentful/rich-text-react-renderer": "^15.4.0",
    "@contentful/rich-text-types": "^15.3.6",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.12.3",
    "@svgr/webpack": "^5.5.0",
    "@types/node": "^16.10.3",
    "@types/react": "^17.0.27",
    "accept-language": "^3.0.18",
    "algoliasearch": "^4.10.5",
    "array-sort": "^1.0.0",
    "color": "^4.0.1",
    "contentful": "^9.0.3",
    "contentful-typescript-codegen": "^3.2.3",
    "cookie": "^0.4.1",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.4",
    "disqus-react": "^1.1.2",
    "eslint-plugin-prettier": "^4.0.0",
    "graphql": "^15.6.1",
    "graphql-hooks-memcache": "^2.2.1",
    "graphql-request": "^3.5.0",
    "graphql-tag": "^2.12.5",
    "html-react-parser": "^1.4.0",
    "htmlhint": "^0.15.2",
    "husky": "^7.0.2",
    "isomorphic-unfetch": "^3.1.0",
    "lodash": "^4.17.21",
    "next": "^12.0.2",
    "next-seo": "^4.28.1",
    "preact": "^10.5.14",
    "preact-render-to-string": "^5.1.19",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-highlight-words": "^0.17.0",
    "react-icons": "^4.3.1",
    "react-instantsearch-dom": "^6.12.1",
    "react-lite-youtube-embed": "^2.0.14-1",
    "react-lowlight": "^2.0.0",
    "react-merge-refs": "^1.1.0",
    "react-query": "^3.25.1",
    "react-share": "^4.4.0",
    "react-swipeable": "^6.2.0",
    "requestidlecallback-polyfill": "^1.0.2",
    "rss": "^1.2.2",
    "shallowequal": "^1.1.0",
    "simple-git": "^2.46.0",
    "snyk": "^1.864.0",
    "typescript": "^4.4.3",
    "url-loader": "^4.1.1",
    "use-http": "^1.0.24",
    "web-vitals-react-hook": "^1.0.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.2",
    "@percy/cli": "^1.0.0-beta.68",
    "@percy/storybook": "^4.0.3",
    "@schoenwald/eslint-config-typescript": "^5.0.1",
    "@storybook/addon-actions": "^6.3.10",
    "@storybook/addon-docs": "^6.3.10",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addon-links": "^6.3.10",
    "@storybook/addon-viewport": "^6.3.10",
    "@storybook/addons": "^6.3.10",
    "@storybook/react": "^6.3.10",
    "@types/color": "^3.0.2",
    "@types/cookie": "^0.4.1",
    "@types/react-instantsearch-dom": "^6.12.0",
    "@types/rss": "0.0.29",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "cli-real-favicon": "^0.0.8",
    "contentful-management": "^7.42.1",
    "dotenv-flow": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.0.2",
    "lint-staged": "^11.2.0",
    "prettier": "^2.4.1",
    "stylelint": "^13.13.1",
    "stylelint-config-schoenwaldnils": "^4.0.0",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "NODE_ENV=production lint-staged && npm run lint:types"
    }
  },
  "lint-staged": {
    "**/*.(ts|tsx)": [
      "eslint --fix"
    ]
  },
  "snyk": true
}
