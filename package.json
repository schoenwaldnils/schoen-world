{
  "name": "schoen-world",
  "version": "0.0.1",
  "description": "Schoen.World",
  "main": "index.js",
  "scripts": {
    "dev": "next dev -p 3002",
    "start": "next build && next start -p 3002",
    "prebuild": "npm run generate-contentful-types",
    "build": "next build",
    "export": "NODE_ENV=production next build && next export",
    "test": "npm run lint",
    "lint": "npm run lint:ts && npm run lint:types",
    "lint:css": "stylelint '**/*.tsx'",
    "lint:ts": "eslint . --ext .ts,.tsx",
    "lint:ts-fix": "eslint . --ext .ts,.tsx --fix",
    "lint:types": "tsc",
    "lint:html": "htmlhint out",
    "generate-contentful-types": "contentful-typescript-codegen --output src/@types/generated/contentful.d.ts",
    "generate-favicons": "real-favicon generate faviconDescription.json src/components/Favicons/faviconData.json public/images/favicons",
    "storybook": "start-storybook -p 6007",
    "build-storybook": "build-storybook -o public/storybook --quiet",
    "snapshot": "build-storybook && percy-storybook --widths=320 -t=500",
    "analyze": "cross-env ANALYZE=true next build",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [
    "next.js",
    "nextjs",
    "contentful",
    "static"
  ],
  "repository": "github:schoenwaldnils/schoen-world",
  "author": {
    "name": "Nils Sch√∂nwald",
    "email": "nils@schoen.world"
  },
  "license": "ISC",
  "dependencies": {
    "@babel/core": "7.14.6",
    "@contentful/rich-text-react-renderer": "15.0.0",
    "@contentful/rich-text-types": "15.0.0",
    "@emotion/react": "11.4.0",
    "@emotion/styled": "11.3.0",
    "@material-ui/core": "4.11.4",
    "@svgr/webpack": "5.5.0",
    "@types/node": "16.3.0",
    "@types/react": "17.0.11",
    "accept-language": "3.0.18",
    "algoliasearch": "4.9.3",
    "array-sort": "1.0.0",
    "color": "3.1.3",
    "contentful": "8.4.2",
    "contentful-typescript-codegen": "3.2.2",
    "cookie": "0.4.1",
    "cross-env": "7.0.3",
    "cross-fetch": "3.1.4",
    "disqus-react": "1.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "graphql": "15.5.1",
    "graphql-hooks-memcache": "2.1.3",
    "graphql-request": "3.4.0",
    "graphql-tag": "2.12.4",
    "html-react-parser": "1.2.7",
    "htmlhint": "0.15.1",
    "husky": "6.0.0",
    "isomorphic-unfetch": "3.1.0",
    "lodash": "4.17.21",
    "next": "11.0.1",
    "next-seo": "4.26.0",
    "preact": "10.5.13",
    "preact-render-to-string": "5.1.19",
    "qs": "6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-highlight-words": "0.17.0",
    "react-icons": "4.2.0",
    "react-instantsearch-dom": "6.12.0",
    "react-lite-youtube-embed": "2.0.3",
    "react-lowlight": "2.0.0",
    "react-merge-refs": "1.1.0",
    "react-query": "3.21.0",
    "react-share": "4.4.0",
    "react-swipeable": "6.1.2",
    "requestidlecallback-polyfill": "1.0.2",
    "rss": "1.2.2",
    "shallowequal": "1.1.0",
    "simple-git": "2.40.0",
    "snyk": "1.657.0",
    "typescript": "4.3.4",
    "url-loader": "4.1.1",
    "use-http": "1.0.21",
    "web-vitals-react-hook": "1.0.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "11.0.0",
    "@percy/storybook": "3.3.1",
    "@schoenwald/eslint-config-typescript": "5.0.1",
    "@storybook/addon-actions": "^6.3.1",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/addon-viewport": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@types/color": "3.0.1",
    "@types/cookie": "0.4.0",
    "@types/react-instantsearch-dom": "6.10.0",
    "@types/rss": "0.0.28",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "cli-real-favicon": "0.0.8",
    "contentful-management": "7.25.1",
    "dotenv-flow": "3.2.0",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1",
    "stylelint": "13.13.1",
    "stylelint-config-schoenwaldnils": "3.0.0",
    "stylelint-processor-styled-components": "1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "NODE_ENV=production lint-staged && npm run lint:types"
    }
  },
  "lint-staged": {
    "**/*.(ts|tsx)": [
      "eslint --fix"
    ]
  },
  "snyk": true
}
